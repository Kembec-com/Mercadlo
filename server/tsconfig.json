	"compilerOptions": {
        "strictNullChecks": true,
        "module": "commonjs",
        "esModuleInterop": true,
        "target": "es6",
        "strict": true,
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "./dist",
		"rootDir": "./src", 
        "baseUrl": ".",
